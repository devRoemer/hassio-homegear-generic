{
  "name": "Homegear",
  "version": "dev",
  "slug": "hassio-homegear-generic",
  "description": "Homegear add-on",
  "url": "https://github.com/devRoemer/hassio-homegear-generic",
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "startup": "services",
  "uart": true,
  "udev": true,
  "usb": true,
  "gpio": true,
  "privileged": ["SYS_ADMIN", "SYS_RAWIO"],
  "devices": ["/dev/mem", "/dev/spidev0.0"],
  "map": ["config:rw", "share:rw"],
  "ingress": true,
  "ingress_port": 2001,
  "ports": {
    "2001/tcp": 2001,
    "2002/tcp": 2002,
    "2003/tcp": 2003
  },
  "ports_description": {
    "2001/tcp": "Homegear Web-Interface",
    "2002/tcp": "Homegear port 2002",
    "2003/tcp": "Homegear port 2003"
  }
}
